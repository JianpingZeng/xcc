static void putDC(tab,val)
sVLCtable *tab;
int val;
{
  int absval, size;

  absval = (val<0) ? -val : val;

  if (absval>2047 || (mpeg1 && absval>255))
  {

    sprintf(errortext,"DC value out of range (%d)\n",val);
    error(errortext);
  }


  size = 0;

  while (absval)
  {
    absval >>= 1;
    size++;
  }


  putbits(tab[size].code,tab[size].len);


  if (size!=0)
  {
    if (val>=0)
      absval = val;
    else
      absval = val + (1<<size) - 1;
    putbits(absval,size);
  }
}



void putACfirst(run,val)
int run,val;
{
  if (run==0 && (val==1 || val==-1))
    putbits(2|(val<0),2);
  else
    putAC(run,val,0);
}


void putAC(run,signed_level,vlcformat)
int run,signed_level,vlcformat;
{
  int level, len;
  VLCtable *ptab;

  level = (signed_level<0) ? -signed_level : signed_level;


  if (run<0 || run>63 || level==0 || level>2047 || (mpeg1 && level>255))
  {
    sprintf(errortext,"AC value out of range (run=%d, signed_level=%d)\n",
      run,signed_level);
    error(errortext);
  }

  len = 0;

  if (run<2 && level<41)
  {

    if (vlcformat)
      ptab = &dct_code_tab1a[run][level-1];
    else
      ptab = &dct_code_tab1[run][level-1];

    len = ptab->len;
  }
  else if (run<32 && level<6)
  {

    if (vlcformat)
      ptab = &dct_code_tab2a[run-2][level-1];
    else
      ptab = &dct_code_tab2[run-2][level-1];

    len = ptab->len;
  }

  if (len!=0)
  {
    putbits(ptab->code,len);
    putbits(signed_level<0,1);
  }
  else
  {

    putbits(1l,6);
    putbits(run,6);
    if (mpeg1)
    {

      if (signed_level>127)
        putbits(0,8);
      if (signed_level<-127)
        putbits(128,8);
      putbits(signed_level,8);
    }
    else
    {

      putbits(signed_level,12);
    }
  }
}


